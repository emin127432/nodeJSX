<h1>kursi</h1>


{{#if courses.length}}
{{#each courses}}
<div class="row">
  <div class="col s12 offset-3">
    <div class="card">
      <div class="card-image">
        <img src={{img}} all={{title}}>
      </div>
      <div class="card-content">
        <span class="card-title">{{title}}</span>
        <p class="price">{{price}}</p>
      </div>
      <div class="card-action">
        <a href="/courses/{{id}}">atkrit kurs</a>
      </div>
    </div>
  </div>
</div>
{{/each}}
{{else}}
<p>Kuresv paka net</p>
{{/if}}